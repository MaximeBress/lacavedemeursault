{
  "arrowParens": "always",
  "singleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 120,
  "importOrder": ["<THIRD_PARTY_MODULES>", "^@(/app|/components)/(.*)$", "^~/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"]
}
